{
  "name": "i18nexus-tools",
  "version": "1.7.6",
  "description": "CLI tools for i18nexus - automate i18n workflows with type-safe configuration and Google Sheets integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "i18n-wrapper": "dist/bin/i18n-wrapper.js",
    "i18n-wrapper-swc": "dist/bin/i18n-wrapper-swc.js",
    "i18n-extractor": "dist/bin/i18n-extractor.js",
    "i18n-upload": "dist/bin/i18n-upload.js",
    "i18n-download": "dist/bin/i18n-download.js",
    "i18n-download-force": "dist/bin/i18n-download-force.js",
    "i18n-sheets": "dist/bin/i18n-sheets.js",
    "i18n-clean-legacy": "dist/bin/i18n-clean-legacy.js"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run postbuild",
    "postbuild": "chmod +x dist/bin/*.js 2>/dev/null || true",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "i18n",
    "cli",
    "internationalization",
    "translation",
    "react",
    "typescript",
    "google-sheets",
    "code-transformation",
    "automation",
    "next.js",
    "server-components",
    "i18next",
    "localization"
  ],
  "author": "i18nexus",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/generator": "^7.23.4",
    "@babel/traverse": "^7.23.4",
    "@babel/types": "^7.23.4",
    "@swc/core": "^1.15.1",
    "commander": "^11.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "glob": "^10.3.10",
    "google-auth-library": "^9.4.0",
    "googleapis": "^129.0.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.7",
    "@types/babel__traverse": "^7.20.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.4",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/i18n-global/i18nexus-tools.git"
  },
  "bugs": {
    "url": "https://github.com/i18n-global/i18nexus-tools/issues"
  },
  "homepage": "https://github.com/i18n-global/i18nexus-tools#readme"
}
